{
  "name": "spoons",
  "version": "2.1.1",
  "private": true,
  "prisma": {
    "seed": "node ./prisma/seed.ts"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "format": "prettier --check --ignore-path .gitignore .",
    "format:fix": "prettier --write --ignore-path .gitignore .",
    "make-gamemaster": "tsx scripts/make-gamemaster.ts",
    "docker:build": "docker build -t docker.bedson.tech/prorickey/spoons:$(node -p \"require('./package.json').version\") -t docker.bedson.tech/prorickey/spoons:latest .",
    "docker:push": "docker push docker.bedson.tech/prorickey/spoons:$(node -p \"require('./package.json').version\") && docker push docker.bedson.tech/prorickey/spoons:latest",
    "docker:release": "npm run docker:build && npm run docker:push"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@react-google-maps/api": "^2.20.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.7",
    "fast-csv": "^5.0.2",
    "lucide-react": "^0.540.0",
    "next": "^16.1.5",
    "next-auth": "^4.24.11",
    "next-themes": "^0.4.6",
    "pg": "^8.17.2",
    "react": "^19.2.4",
    "react-datepicker": "^8.0.0",
    "react-dom": "^19.2.4",
    "react-select": "^5.9.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.0.3",
    "@types/node": "^22",
    "@types/pg": "^8.16.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "^16.1.5",
    "eslint-config-prettier": "^10.0.1",
    "postcss": "^8",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "prisma": "^7.3.0",
    "tailwindcss": "^4",
    "tsx": "^4.19.2",
    "tw-animate-css": "^1.3.7",
    "typescript": "^5"
  }
}
